{
  "version": 2,
  "routes": [
    {
      "src": "/",
      "dest": "/server.js",
      "headers": {
        "key": "access-control-allow-origin",
        "value": "*"
      }
    },
    {
      "src": "/addItem",
      "dest": "/server.js",
      "methods": ["OPTIONS", "POST"],
      "headers": {
        "key": "access-control-allow-origin",
        "value": "*"
      }
    },
    {
      "src": "/addReview",
      "dest": "/server.js",
      "methods": ["OPTIONS", "POST"],
      "headers": {
        "key": "access-control-allow-origin",
        "value": "*"
      }
    },
    {
      "src": "/findFood/:id",
      "dest": "/server.js",
      "headers": {
        "key": "access-control-allow-origin",
        "value": "*"
      }
    },
    {
      "src": "/findReview/:id",
      "dest": "/server.js",
      "headers": {
        "key": "access-control-allow-origin",
        "value": "*"
      }
    },
    {
      "src": "/deleteItem/:userId/:itemId",
      "dest": "/server.js",
      "methods": ["OPTIONS", "DELETE"],
      "headers": {
        "key": "access-control-allow-origin",
        "value": "*"
      }
    },
    {
      "src": "/userLogin",
      "dest": "/server.js",
      "methods": ["OPTIONS", "POST"],
      "headers": {
        "key": "access-control-allow-origin",
        "value": "*"
      }
    },
    {
      "src": "/userRegister",
      "dest": "/server.js",
      "methods": ["OPTIONS", "POST"],
      "headers": {
        "key": "access-control-allow-origin",
        "value": "*"
      }
    }
  ]
}
